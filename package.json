{
  "name": "pocket-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2-runtime src -i -1 --node-args='-r esm'",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js --watch src/ --trace-deprecation",
    "start:pm2": "cross-env NODE_ENV=production pm2 start src -i -1 --node-args='-r esm'",
    "stop:pm2:dev": "cross-env NODE_ENV=development pm2 kill",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --forceExit --silent",
    "test:dev": "cross-env NODE_ENV=test jest --passWithNoTests --forceExit",
    "test:watch": "yarn test:dev --watch --silent"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "get-port": "^5.1.1",
    "http-status": "^1.5.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.1.1",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "mongoose": "^5.9.18",
    "nodemailer": "^6.7.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "open-graph-scraper": "^4.11.0",
    "pm2": "^5.1.2",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "babel-jest": "^27.3.1",
    "chai": "^4.3.4",
    "chai-string": "^1.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.4.2",
    "jest-esm-transformer": "^1.0.0",
    "mocha": "^9.1.3",
    "mongodb-memory-server": "^8.0.2",
    "mongodb-memory-server-global": "^8.0.2",
    "nodemon": "^2.0.15",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6"
  }
}
