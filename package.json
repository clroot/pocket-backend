{
  "name": "pocket-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2-runtime src -i -1 --node-args='-r esm'",
    "start:dev": "cross-env NODE_ENV=development nodemon --watch src/ --trace-deprecation -r esm src/index.js",
    "test": "cross-env NODE_ENV=test jest --passWithNoTests --forceExit --runInBand --silent",
    "test:dev": "cross-env NODE_ENV=test jest --passWithNoTests --forceExit",
    "test:watch": "yarn test:dev --watch --silent"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "get-port": "^5.1.1",
    "http-status": "^1.4.2",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "mongoose": "^5.9.18",
    "nodemailer": "^6.4.13",
    "nodemailer-smtp-transport": "^2.7.4",
    "open-graph-scraper": "^4.1.1",
    "pm2": "^4.4.1",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.4.2",
    "jest-esm-transformer": "^1.0.0",
    "mocha": "^8.1.3",
    "mongodb-memory-server": "^6.8.1",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0"
  }
}
